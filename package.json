{
  "name": "eslint-linter-browserify",
  "version": "8.45.0",
  "description": "Use eslint in the browser",
  "main": "./linter.cjs",
  "module": "./linter.mjs",
  "browser": "./linter.js",
  "files": [
    "index.d.ts",
    "linter.js",
    "linter.min.js",
    "linter.mjs",
    "linter.cjs"
  ],
  "types": "index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./linter.mjs"
      },
      "default": "./linter.cjs"
    },
    "./linter.js": "./linter.js",
    "./linter.min.js": "./linter.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UziTech/eslint-linter-browserify.git"
  },
  "keywords": [
    "eslint",
    "lint",
    "linter",
    "browser",
    "browserify"
  ],
  "author": "Tony Brix <tony@brix.ninja>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UziTech/eslint-linter-browserify/issues"
  },
  "homepage": "https://github.com/UziTech/eslint-linter-browserify#readme",
  "scripts": {
    "test": "node test.cjs && node test.mjs",
    "build": "rollup --config",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "eslint": "8.45.0",
    "process": "^0.11.10",
    "rollup": "^3.26.2",
    "rollup-plugin-node-builtins": "^2.1.2"
  }
}
